=extends

=rules
- array-bracket-eol = on
- operators-spacing = on
- comma-spacing = on
- indent = 1
- comments-header = on
- max-line-numbers = 150
- max-file-line-numbers = 600
- no-trailing-spaces = on
- no-multi-declaration = on
- unused-variable = off
- undeclared-variable = off
- no-prototype = off
- unused-function = off
- undeclared-function = off
- variable-assignment-type = off
- function-parameters-type = off

=excludedFiles

=recursive
true
